# Function: p6df::user::prompt()
#
#>
######################################################################
p6df::user::prompt() {

  p6df::core::prompt::line::add::mod::bottom "p6df::modules::zsh::std::prompt::mod"
  p6df::core::prompt::line::add::mod::bottom "p6df::modules::zsh::dir::prompt::mod"

  p6_return_void
}

