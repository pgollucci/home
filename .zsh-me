##############################################################################
p6df::user::prompt() {

  p6df::core::prompt::line::add "p6df::modules::zsh::std::prompt::line"
  p6df::core::prompt::line::add "p6df::modules::zsh::dir::prompt::line"
  p6df::core::prompt::line::add "p6df::modules::rb::env::prompt::info"
  p6df::core::prompt::line::add "p6df::modules::pipenv::prompt::info"
  p6df::core::prompt::line::add "p6df::modules::js::pm::prompt::info"
}

