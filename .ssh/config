ForwardAgent yes
ForwardX11 no
CheckHostIP no
StrictHostKeyChecking no

XAuthLocation /usr/local/bin/xauth

Host github.com
        UseKeychain yes
        AddKeysToAgent yes
	User pgollucci
	IdentityFile ~/.ssh/id_rsa-p6m7g8.com-2

Host *.apache.org
        UseKeychain yes
        AddKeysToAgent yes
	User pgollucci
	IdentityFile ~/.ssh/id_rsa-apache.org-2

Host *.freebsd.org
        UseKeychain yes
        AddKeysToAgent yes
	User pgollucci
	IdentityFile ~/.ssh/id_rsa-freebsd.org-2

Host *.p6m7g8.net
        UseKeychain yes
        AddKeysToAgent yes
	User pgollucci
	IdentityFile ~/.ssh/id_rsa-p6m7g8.com-2
