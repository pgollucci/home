[color]
	diff = auto
	status = auto
	branch = auto
[alias]
	br = branch
	ba = branch -a
	tg = tag
	ci = commit
	cv = commit -v
	co = checkout
	dump = cat-file -p
	lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
	st = status
	stat = status
	stauts = status
	type = cat-file -t
[github]
	ssl = true

[user]
	name = Philip M. Gollucci
	email = pgollucci@p6m7g8.com
[core]
	pager = cat
[push]
	default = simple

[remote "origin"]
  fetch = +refs/pull/*/head:refs/remotes/origin/pr/*
